<div id="new">
	<h2>Add new friend</h2>
	<div id="add">
		<%= form_for(@friend) do |f| %>
			<p>
				<%= f.label :first_name %></br>
				<%= f.text_field :first_name %>
			</p>
			<p>
				<%= f.label :last_name %></br>
				<%= f.text_field :last_name %>
			</p>
			<p>
				<%= f.label :photo %></br>
				<%= f.text_field :photo %>
			</p>
			<p>
				<%= f.submit %>
			</p>
		<% end %>
		<%= link_to "Back", friends_path, class: "back" %>
	</div>
</div>

